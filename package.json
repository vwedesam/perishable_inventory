{
  "name": "perishable_inventory_server",
  "version": "1.0.0",
  "description": "A Nodejs Server for managing perishable inventory",
  "author": "SamVwede",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "type": "commonjs",
  "scripts": {
    "clean": "rm -rf dist*",
    "db:cleanup": "node ./dist/cron-jobs/cleanup.js",
    "migrate": "npx prisma migrate dev --schema=./src/prisma/schema.prisma ",
    "generate": "npx prisma generate --schema=./src/prisma/schema.prisma ",
    "dev": "npm run generate && npm run watch & NODE_ENV=development DEBUG=express:* nodemon .",
    "build": "tsc --project './tsconfig.json'",
    "start": "npm run build && NODE_ENV=production node .",
    "watch": "tsc --project './tsconfig.json' --watch",
    "postinstall": "npm run generate",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix --ext src/**/*.ts",
    "test": "jest",
    "test:e2e": "jest --testRegex='e2e.test.(ts|js)$' "
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.2.0",
    "helmet": "^6.0.1",
    "joi": "^17.12.2",
    "node-cron": "^3.0.3",
    "reflect-metadata": "^0.2.1",
    "winston": "^3.8.2",
    "winston-syslog": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@jest-mock/express": "^2.0.2",
    "@jest/globals": "^29.7.0",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-jest": "^29.7.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^2.0.21",
    "prisma": "^5.10.2",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx eslint --fix"
    ]
  }
}
